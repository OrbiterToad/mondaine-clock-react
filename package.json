{
  "name": "mondaine-clock-react",
  "type": "module",
  "version": "2.0.0",
  "private": false,
  "scripts": {
    "start": "react-scripts start",
    "test": "react-scripts test",
    "build": "rm -rf dist && NODE_ENV=production tsc --jsx react-jsx --outDir ./bongo ./src/lib/** && babel bongo --out-dir dist --copy-files --ignore __tests__,spec.js,test.js,__snapshots__",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/react": "^18.2.25",
    "react-scripts": "5.0.1",
    "typescript": "^4.9.5"
  },
  "devDependencies": {
    "@types/react-test-renderer": "^18.0.3",
    "babel-cli": "^6.26.0",
    "react-test-renderer": "^18.0.3",
    "react": ">=16.0.1",
    "react-dom": ">=16.0.1"
  },
  "peerDependencies": {
    "react": ">=16.0.1",
    "react-dom": ">=16.0.1"
  }
}
